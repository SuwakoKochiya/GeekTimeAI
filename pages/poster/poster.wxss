.posterpage {
    width: 100%;
    height: 100vh;
    overflow: hidden;
    position: relative;
    background-image: url('data:image/jpg;base64,');
    background-repeat: repeat;
    background-size: 7%;
}

.canvas1 {
    width: 100%;
    height: 100%;
}

.canvas2 {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.shakeCanvas1 {
    width: 100%;
    height: 100%;
    overflow: hidden;
    visibility: hidden;
}

.shakeCanvas2 {
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.saveBtn {
    width: 55%;
    height: 57px;
    position: absolute;
    left: 23%;
    top: 81%;
}